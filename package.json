{
  "name": "figma-to-code-indieappstudio",
  "version": "1.0.0",
  "description": "react",
  "main": "dist/code.js",
  "scripts": {
    "test": "npm run tsc && npm run build",
    "format": "prettier --write .",
    "tsc": "npm run tsc:main && npm run tsc:ui",
    "tsc:main": "tsc --noEmit",
    "tsc:ui": "tsc --noEmit",
    "tsc:watch": "concurrently -n backend,ui \"npm run tsc:main -- --watch --preserveWatchOutput\" \"npm run tsc:ui -- --watch --preserveWatchOutput\"",
    "build": "npm run build:ui && npm run build:main -- --minify",
    "build:main": "esbuild plugin/code.ts --bundle --target=ES6 --outfile=dist/code.js",
    "build:ui": "npx vite build --minify esbuild --emptyOutDir=false",
    "build:watch": "concurrently -n backend,ui \"npm run build:main -- --watch\" \"npm run build:ui -- --watch\"",
    "dev": "concurrently -n tsc,build,vite 'npm:tsc:watch' 'npm:build:watch' 'vite'"
  },
  "author": "Indie App Studio",
  "dependencies": {
    "@amplitude/analytics-browser": "^2.9.3",
    "@builder.io/sdk": "^2.2.6",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mojotech/json-type-validation": "^3.1.0",
    "@mui/icons-material": "^5.16.4",
    "@mui/material": "^5.16.4",
    "@starptech/prettyhtml-hast-to-html": "^0.6.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "deepmerge": "^4.3.1",
    "file-type": "^19.2.0",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "mobx": "^6.13.0",
    "mobx-react": "^9.1.1",
    "pako": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "traverse": "^0.6.9",
    "uuid": "^10.0.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.24.7",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@figma/plugin-typings": "*",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.0.0",
    "@types/pako": "^2.0.3",
    "@types/react": "^18.2.38",
    "@types/react-dom": "^18.2.16",
    "@types/traverse": "^0.6.37",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.7",
    "esbuild-plugins-node-modules-polyfill": "^1.6.4",
    "prettier": "3.3.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.3.2",
    "util": "^0.12.5",
    "vite": "^5.0.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-svg-loader": "^5.1.0"
  }
}
